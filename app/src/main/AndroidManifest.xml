<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cecel.wfwpp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.DELE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wfwpp">
        <activity
            android:name=".CourseList"
            android:label="所有课程信息"/>
        <activity
            android:name=".CourseWebview"
            android:label="教务处" />
        <activity
            android:name=".CourseTable"
            android:label="个人课表" />
        <activity
            android:name=".BigStudy"
            android:label="青年大学习" />
        <activity android:name=".CopyrightPage" />
        <activity
            android:name=".LostCardOption"
            android:label="校园卡挂失招领" />
        <activity
            android:name=".SchoolNetOption"
            android:label="校园卡相关" />
        <activity
            android:name=".AutoSign"
            android:label="健康不用每日报" />
        <activity
            android:name=".wfwWebView"
            android:usesCleartextTraffic="true" />
        <activity
            android:name=".LoginActivity"
            android:label="登录页面" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>